generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model todo {
  id          Int     @id @default(autoincrement())
  title       String  @db.VarChar(100)
  description String? @db.LongText
  user_id     Int
  completed   Boolean @default(false)
}
